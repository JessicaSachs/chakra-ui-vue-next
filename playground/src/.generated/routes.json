[
  {
    "name": "Home",
    "path": "/",
    "component": "../components/Home.vue"
  },
  {
    "name": "Alert",
    "path": "/c-alert",
    "children": [
      {
        "name": "Base alert",
        "path": "/c-alert/base-alert",
        "component": "@chakra-ui/c-alert/examples/base-alert.vue"
      },
      {
        "name": "With accent",
        "path": "/c-alert/with-accent",
        "component": "@chakra-ui/c-alert/examples/with-accent.vue"
      },
      {
        "name": "With status",
        "path": "/c-alert/with-status",
        "component": "@chakra-ui/c-alert/examples/with-status.vue"
      },
      {
        "name": "With title",
        "path": "/c-alert/with-title",
        "component": "@chakra-ui/c-alert/examples/with-title.vue"
      }
    ]
  },
  {
    "name": "Button",
    "path": "/c-button",
    "children": [
      {
        "name": "Base button",
        "path": "/c-button/base-button",
        "component": "@chakra-ui/c-button/examples/base-button.vue"
      }
    ]
  },
  {
    "name": "Icon",
    "path": "/c-icon",
    "children": [
      {
        "name": "Base icon",
        "path": "/c-icon/base-icon",
        "component": "@chakra-ui/c-icon/examples/base-icon.vue"
      }
    ]
  },
  {
    "name": "NotFound",
    "path": "/*",
    "component": "../components/Home.vue"
  }
]